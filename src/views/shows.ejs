<!-- views/shows.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Shows</title>
</head>
<body>
    <h1>List of Shows</h1>
    <ul>
        <% showList.forEach(show => { %>
            <li><%= show.name %></li>
        <% }); %>
        <% showList.map(item => { %>
            <li><%= item.name %></li>
        <% }); %>
    </ul>
</body>
</html>
